{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "ckeditor5@38.1.1",
      "destination": "wwwroot/lib/ckeditor5/"
    },
    {
      "library": "paginationjs@2.6.0",
      "destination": "wwwroot/lib/paginationjs/"
